<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fit.upa.MultiOBJ">

    <children>
        <Text fx:id="name" layoutX="135.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
            <font>
                <Font size="18.0" />
            </font>
        </Text>

        <Button fx:id="backImg" layoutX="210.0" layoutY="50.0" mnemonicParsing="false" onAction="#backImg" text="back" />
        <Button fx:id="addImg" layoutX="70.0" layoutY="50.0" mnemonicParsing="false" onAction="#addImg" text="Add" />

        <Button fx:id="rotateImgLeft" layoutX="330.0" layoutY="110.0" mnemonicParsing="false" onAction="#rotateImgLeft" text="R~left" />
        <Button fx:id="rotateImgRight" layoutX="330.0" layoutY="160.0" mnemonicParsing="false" onAction="#rotateImgRight" text="R~right" />
        <Button fx:id="findSim" layoutX="330.0" layoutY="210.0" mnemonicParsing="false" onAction="#findSim" text="Similar" />
        <Button fx:id="delImg" layoutX="330.0" layoutY="260.0" mnemonicParsing="false" onAction="#delImg" text="Delete" visible="false"/>

        <Button fx:id="prev" layoutX="70.0" layoutY="320.0" mnemonicParsing="false" onAction="#prev" text="Prev" visible="false" />
        <Button fx:id="next" layoutX="210.0" layoutY="320.0" mnemonicParsing="false" onAction="#next" text="Next"  visible="false"/>
    </children>

    <AnchorPane fx:id="imageOut" layoutX="20.0" layoutY="100.0" minHeight="200.0" minWidth="300.0" maxHeight="200.0" maxWidth="300.0"/>
