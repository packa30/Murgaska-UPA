    <AnchorPane fx:id="imageSim3" layoutX="185.0" layoutY="420.0" minHeight="50.0" minWidth="50.0" maxHeight="50.0" maxWidth="50.0" visible="false" onMouseClicked="#imageSimHandler"/>
    <AnchorPane fx:id="imageSim4" layoutX="265.0" layoutY="420.0" minHeight="50.0" minWidth="50.0" maxHeight="50.0" maxWidth="50.0" visible="false" onMouseClicked="#imageSimHandler"/>
    <AnchorPane fx:id="imageSim" layoutX="20.0" layoutY="480.0" minHeight="200.0" minWidth="300.0" maxHeight="200.0" maxWidth="300.0" visible="false" onMouseClicked="#imageSimHandler"/>


